procedur hostcwrulogin 
timeout 60 'cwruts2 termin server responding' 
messag "cwruts2 slip login script version 11" 
messag 'wait slip server respond' 
quiet 
expect 'verification' 
messag 'request user verif receiv cwruts2' 
messag 'send user name password' 
quiet 
expect 'username' 
send 'u' 
expect 'password' 
privat 
send 'p' 
reject 'access denied' 'your user name password accepted' 
timeout 30 'slip server respond valid request' 
expect 'cwruts2' 
send 'slip' 
timeout 10 'slip server respond slip command' 
expect 'ip hostnam address' 
send 'uslipdialincwruedu' 
timeout 10 'slip server respond hostname' 
reject 'bad ip address' 'incorrect hostname' 
expect 'password' 
send 'p' 
reject 'access denied' 'password accepted' 
timeout 10 
expect 'header compress will match system' 
messag 'login cwru slip server successful' 
wait 10 
endprocedur hostcwrulogin 


